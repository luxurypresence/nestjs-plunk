{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": "./",
  "testEnvironment": "node",
  "testMatch": [
    "**/*.spec.ts"
 ],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^lib/(.*)$": "<rootDir>/lib/$1"
  },
  "detectOpenHandles": true
}
